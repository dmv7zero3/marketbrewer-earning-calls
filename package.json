{
  "name": "marketbrewer-earnings-call",
  "version": "0.1.0",
  "private": true,
  "description": "Personal earnings call betting assistant with Kalshi integration",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "build:clean": "rm -rf dist && bun run build",
    "server": "bun run server/index.ts",
    "dev:all": "concurrently \"bun start\" \"bun run server\"",
    "test": "bun test",
    "test:unit": "bun test tests/unit/",
    "test:smoke": "bun test tests/smoke.test.ts",
    "test:watch": "bun test --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "deploy": "bun run build && bun run scripts/deploy.mjs",
    "lambda:deploy": "cd infrastructure && sam build && sam deploy"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.971.0",
    "@aws-sdk/lib-dynamodb": "^3.971.0",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.17",
    "cheerio": "^1.1.2",
    "concurrently": "^8.2.2",
    "css-loader": "^6.9.1",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.7",
    "postcss": "^8.4.33",
    "postcss-loader": "^8.0.0",
    "prettier": "^3.2.4",
    "puppeteer": "^24.35.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
}
